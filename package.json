{
  "name": "gemini-party",
  "version": "1.1.0",
  "module": "index.ts",
  "dependencies": {
    "hono": "^4.7.5",
    "openai": "^4.92.1"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "private": true,
  "scripts": {
    "start": "bun ./src/index.ts",
    "docker-build": "docker build --pull -t gemini-party .",
    "bun-build": "bun build ./src/index.ts --outfile ./serverless/bun.js --target=node --external hono --external openai",
    "deno-build": "bun build ./src/index.ts --outfile ./serverless/deno.js --target=node --external hono --external openai && node scripts/deno-modify-imports.js",
    "release": "node scripts/release.js",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "push": "git push && git push --tags"
  },
  "type": "module",
  "devDependencies": {
    "prettier": "^3.5.3"
  }
}
